	function DateDiff(date1, date2) {
    var datediff = date1.getTime() - date2.getTime(); //store the getTime diff - or +
    return (datediff / (24*60*60*1000)); //Convert values to -/+ days and return value      
	}
// checks date, fromdate and todate with european validation
function isDate(dat, from, to, eur)
{
	var ret = false;
	try
	{
		var i;

		// convert date to mm/dd/yyyy, setup date array and test date
		dat = convertDate($.trim(dat), eur);
		var datarray = dat.split("/");
		var testdat = new Date(Date.parse(dat));
		if (datarray[0] != (testdat.getMonth()+1) || datarray[1] != testdat.getDate() || datarray[2] != testdat.getFullYear() || datarray[0] == '' || datarray[1] == '' || datarray[2] == '')
		{
			return ret;
		}
		
		// test for valid date, from date and to date.
		ret = !((from && Date.parse(dat) < Date.parse(convertDate(from, eur))) || (to && Date.parse(dat) > Date.parse(convertDate(to, eur))));
	}
	catch(e){}
	
	return ret;
}

function convertDate(dt, eur)
{
	try
	{
		if(dt != "" && dt.replace(/\d\d\d\d\d\d/, "") == "")
		{
			dt = dt.substr(0, 2) + "/" + dt.substr(2, 2) + "/20" + dt.substr(4, 2);
			return dt;
		}
		var i;
		var m = new Array("jan", "feb", "mar", "apr", "may", "jun", "jul", "aug", "sep", "oct", "nov", "dec");
		var oldDt = (dt == "")? " " : dt;

		// format the date into mm/dd/yyyy or dd/mm/yyyy
		dt = dt.replace(/[\.\/,\\-]/g, " ").replace(/[\s]+/g, "/");
		var datarray = dt.split("/");
		
		// converts year to fullyear
		if(typeof datarray[2] != "undefined")
		{
			datarray[2] = (datarray[2].length != 2)? datarray[2] : (parseInt(datarray[2]) < 49)? "20" + datarray[2] : "19" + datarray[2];
		}
		  
		// converts dates from dd/mm/yyyy to mm/dd/yyyy
		if (eur)
		{
			var d = datarray[0];
			datarray[0] = datarray[1];
			datarray[1] = d;
		}
		  
		// looks for months entered as strings and converts them to a number
		if (isNaN(datarray[0]) && typeof datarray[0] != 'undefined')
		{
			for (i = 0; i < 12; i++ )
			{
				if (datarray[0].toLowerCase().indexOf(m[i]) != -1)
				{
					dt = (i + 1) + "/" + datarray[1] + "/" + datarray[2];
					break;
				}
			}
		}
		else
		{
			dt = datarray[0] + "/" + datarray[1] + "/" + datarray[2];
		}
		
		dt = (dt.indexOf("undefined") == -1)? dt : oldDt;	
	}
	catch(e){}
	
	return dt;
}

	function calculateTaxProration() {
		var taxprorationamount = 0;
		//var dates = $("#ctl00_content_cph_CountyTaxesDate").val();
		//var datesSplit = dates.split(" ");
		var dt = new Date();
		var fromDate = "1/1/" + dt.getFullYear();
		var toDate = (dt.getMonth() + 1) + "/" + dt.getDate() + "/" + dt.getFullYear();
		
		//if(datesSplit.length > 1) {
		//	fromDate = datesSplit[0];
		//	toDate = datesSplit[datesSplit.length - 1];
		//}
		
		var settlementdate = $.trim($("#ctl00_content_cph_SettlementDate").val());
		if(settlementdate != "" && isDate(settlementdate)) {
			toDate = settlementdate;
		}
		
		var datediff = Math.abs(Math.round(DateDiff(new Date(fromDate), new Date(toDate))));
		
		var taxpercent = 1.05
		var taxpercentTxt = $("#ctl00_content_cph_TaxProrationPercent").val();
		if(taxpercentTxt != "" && isNaN(taxpercentTxt)) {
			taxpercent = parseFloat(taxpercentTxt);
		}

		var taxproration = datediff / 365;
		var city = $("#ctl00_content_cph_PropertyCity").val().toLowerCase();
		var tax = $("#ctl00_content_cph_CountyTaxLastYearBilledAmount").val();
		if(typeof cookcounty[city] != "undefined") {
			taxprorationamount = ((tax *.45) + tax) * (taxproration); //+ (Fixed Proration to add)
 		}
		else {
 			taxprorationamount = tax * taxproration;// + (Fixed Proration to add);
 		}
 		taxprorationamount = Math.round(taxprorationamount * (100 * taxpercent)/100);
 		
 		$("#ctl00_content_cph_TaxProrationCredit").val(taxprorationamount);
	}
</script>
    
