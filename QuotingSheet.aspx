<script>
	var origSimultaneousCharge = 0;
	$(function(){
		$("#ctl00_content_cph_PurchasePrice").bind("change", function() {
			var amtStr = $("#ctl00_content_cph_PurchasePrice").val();
			var loanAmtStr = $("#ctl00_content_cph_LoanAmount").val();
			var amt = (amtStr != "" && !isNaN(amtStr))? parseFloat(amtStr) : 0;
			
			$.ajax("/QuotingSheet.aspx?PurchasePrice=" + amt, {
						dataType: "html",
			      success: function(data) {
			      	//prompt("" ,data)
			      	$("#ctl00_content_cph_FullOwnersPolicyPremium").val(data);
			      	CalculateDisclosedOwnersPolicyPremium();
			      }
   		});
		});
		$("#ctl00_content_cph_LoanAmount").bind("change", function() {
			$.ajax("/QuotingSheet.aspx?LoanAmount=" + this.value, {
						dataType: "html",
			      success: function(data) {
			      	//prompt("" ,data)
			      	$("#ctl00_content_cph_FullLoanPolicy").val(data);
			      	CalculateDisclosedOwnersPolicyPremium();
			      }
   		});
		});
		
		CalculateDisclosedOwnersPolicyPremium();
	});
	
	function CalculateDisclosedOwnersPolicyPremium() {
		var premAmtStr = $("#ctl00_content_cph_FullOwnersPolicyPremium").val();
		var premAmt = (premAmtStr != "" && !isNaN(premAmtStr))? parseFloat(premAmtStr) : 0;
		
		var simAmtStr = $("#ctl00_content_cph_SimultaneousCharge").val();
		var simAmt = (simAmtStr != "" && !isNaN(simAmtStr))? parseFloat(simAmtStr) : 0;
		if(origSimultaneousCharge == 0) {
			origSimultaneousCharge = simAmt;
		}
		
		var policyAmtStr = $("#ctl00_content_cph_FullLoanPolicy").val();
		var policyAmt = (policyAmtStr != "" && !isNaN(policyAmtStr))? parseFloat(policyAmtStr) : 0;
		
		if(policyAmt > premAmt) {
			if(origSimultaneousCharge == 0) {
				origSimultaneousCharge = simAmt;
			}
			simAmt = origSimultaneousCharge + (policyAmt - premAmt);
		}
		else {
			simAmt = origSimultaneousCharge;
		}
		$("#ctl00_content_cph_SimultaneousCharge").val(simAmt);
		
		var policyPrem = Math.round(((premAmt + simAmt) - policyAmt) * 100) / 100;
		var policyPremStr = policyPrem.toString();
		var policyPremArr = policyPremStr.split(".");
		
		if(policyPremArr.length == 1) {
			policyPremStr = policyPremStr + ".00";
		}
		else if(policyPremArr[1].length == 1) {
			policyPremStr = policyPremStr + "0";
		}
		
		var premCredit = premAmt - policyPrem;
		var premCreditStr = premCredit.toString();
		var premCreditArr = premCreditStr.split(".");
		
		if(premCreditArr.length == 1) {
			premCreditStr = premCreditStr + ".00";
		}
		else if(premCreditArr[1].length == 1) {
			premCreditStr = premCreditStr + "0";
		}
		
		$("#ctl00_content_cph_DisclosedOwnersPolicyPremium, #ctl00_content_cph_LECDTitleOwnersPolicy").val(policyPremStr);
		$("#ctl00_content_cph_LECDTitleLoanPolicy").val(policyAmtStr);
		$("#ctl00_content_cph_LECDPage3SectionLSPremiumCredit").val(premCreditStr);
		$("#ctl00_content_cph_SettlementStatementLoanPolicy").val(simAmtStr);
		$("#ctl00_content_cph_SettlementStatementOwnersPolicy").val(premAmtStr);
	}
</script>
    
